[
  {
    "corsRuleName": "downloadFromAnyOriginWithUpload",
    "allowedOrigins": [
      "https://localhost:5173",
      "https://localhost:8080",
      "https://hudori.app",
      "https://api.hudori.app"
    ],
    "allowedHeaders": [
      "authorization",
      "content-type",
      "x-bz-file-name",
      "x-bz-content-sha1"
    ],
    "allowedOperations": [
      "b2_download_file_by_id",
      "b2_download_file_by_name",
      "b2_upload_file",
      "b2_upload_part"
    ],
    "maxAgeSeconds": 3600
  }
]
